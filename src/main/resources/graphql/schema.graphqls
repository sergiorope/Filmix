type Query {
    getRecommended(ids: [ID!]!): [PeliculaDTO]    
    obtenerUsuario: UsuarioDTO
}

type PeliculaDTO {
    id: ID!
    nombre: String
    sinopsis: String
    imagen: String
    listaCategorias: [String]
    listaValoraciones: [String]
}

type UsuarioDTO {
    id: ID!
    nombre: String
    correo: String
    password: String
    lista: Int
 
}